<h1>Listing workout_templates</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Athlete</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @workout_templates.each do |workout_template| %>
  <tr>
    <td><%= workout_template.name %></td>
    <td><%= workout_template.athlete_id %></td>
    <td><%= link_to 'Show', workout_template %></td>
    <td><%= link_to 'Edit', edit_workout_template_path(workout_template) %></td>
    <td><%= link_to 'Destroy', workout_template, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Workout template', new_workout_template_path %>
